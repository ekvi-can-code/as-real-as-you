{
	"schemaVersion": 1,
	"id": "as-real-as-you",
	"version": "${version}",
	"name": "As real as You",
	"description": "Real life in Minecraft",
	"authors": [
		"ekvi"
	],
	"contact": {
		"sources": "https://github.com/ekvi-can-code/as-real-as-you"
	},
	"license": "CC0-1.0",
	"icon": "assets/as-real-as-you/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			{
				"value": "com.ekvicancode.AsRealAsYou",
				"adapter": "kotlin"
			}
		],
		"client": [
			{
				"value": "com.ekvicancode.AsRealAsYouClient",
				"adapter": "kotlin"
			}
		]
	},
	"mixins": [
		"as-real-as-you.mixins.json",
		{
			"config": "as-real-as-you.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.18.4",
		"minecraft": "~1.21.11",
		"java": ">=21",
		"fabric-api": "*",
		"fabric-language-kotlin": "*"
	}
}